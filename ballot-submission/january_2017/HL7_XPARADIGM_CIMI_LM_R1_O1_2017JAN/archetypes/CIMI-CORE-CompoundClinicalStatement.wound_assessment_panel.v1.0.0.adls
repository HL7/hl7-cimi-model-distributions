definition
	CompoundClinicalStatement[id1.1.1.1] matches {
		entry matches {
			use_archetype IndivisibleClinicalStatement [id0.1.0.1, CIMI-CORE-IndivisibleClinicalStatement.wound_area_stmt.v1] occurrences matches {1..1}
			use_archetype IndivisibleClinicalStatement [id0.1.0.2, CIMI-CORE-IndivisibleClinicalStatement.wound_circumference_stmt.v1] occurrences matches {1..1}
			use_archetype IndivisibleClinicalStatement [id0.1.0.3, CIMI-CORE-IndivisibleClinicalStatement.wound_volume_stmt.v1] occurrences matches {1..1}
			use_archetype IndivisibleClinicalStatement [id0.1.0.4, CIMI-CORE-IndivisibleClinicalStatement.periwound_condition_stmt.v1] occurrences matches {1..1}
			use_archetype CompoundClinicalStatement [id0.1.0.5, CIMI-CORE-CompoundClinicalStatement.wound_bed_and_edge_panel.v1] occurrences matches {1..1}
			use_archetype CompoundClinicalStatement [id0.1.0.6, CIMI-CORE-CompoundClinicalStatement.wound_tunneling_undermining_panel.v1] occurrences matches {1..1}
			use_archetype CompoundClinicalStatement [id0.1.0.7, CIMI-CORE-CompoundClinicalStatement.wound_exudate_panel.v1] occurrences matches {1..1}
			use_archetype CompoundClinicalStatement [id0.1.0.8, CIMI-CORE-CompoundClinicalStatement.wound_size_panel.v1] occurrences matches {1..1}
			use_archetype CompoundClinicalStatement [id0.1.0.9, CIMI-CORE-CompoundClinicalStatement.wound_dressing_panel.v1] occurrences matches {1..1}
		}
	}

