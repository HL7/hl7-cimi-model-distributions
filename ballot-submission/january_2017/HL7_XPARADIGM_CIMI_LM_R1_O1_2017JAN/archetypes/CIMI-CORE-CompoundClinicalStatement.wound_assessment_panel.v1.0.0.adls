	term_definitions = <
		["en"] = <
			["id1.1.1.1"] = <
				text = <"Wound assessment panel">
				description = <"-">
			>
			["id0.1.0.1"] = <
				text = <"Wound area">
				description = <"The two-dimensional extent of the wound.">
			>
			["id0.1.0.2"] = <
				text = <"Wound circumference">
				description = <"The distance around the outside edge of the wound.">
			>
			["id0.1.0.3"] = <
				text = <"Wound volume">
				description = <"The volume of missing tissue or the volume of missing and morbid tissue.">
			>
			["id0.1.0.4"] = <
				text = <"Periwound condition">
				description = <"A description of the skin around the wound (periwound). (LOINC).">
			>
			["id0.1.0.5"] = <
				text = <"Wound bed and edge panel">
				description = <"Wound bed and edge panel.">
			>
			["id0.1.0.6"] = <
